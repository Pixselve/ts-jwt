{
  "name": "ts-jwt",
  "version": "8.5.4",
  "description": "JSON Web Token implementation (symmetric and asymmetric)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 95,
    "exclude": [
      "./test/**"
    ],
    "reporter": [
      "json",
      "lcov",
      "text-summary"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "coverage": "nyc mocha --use_strict",
    "test": "npm run lint && npm run coverage && cost-of-modules",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pixselve/ts-jwt"
  },
  "keywords": [
    "jwt",
    "typescript"
  ],
  "author": "auth0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/node-jsonwebtoken/issues"
  },
  "dependencies": {
    "jws": "^3.2.2",
    "lodash.includes": "^4.3.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isinteger": "^4.0.4",
    "lodash.isnumber": "^3.0.3",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "lodash.once": "^4.0.0",
    "ms": "^2.1.1",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "@types/jws": "^3.2.3",
    "@types/lodash.includes": "^4.3.6",
    "@types/lodash.isboolean": "^3.0.6",
    "@types/lodash.isinteger": "^4.0.6",
    "@types/lodash.isnumber": "^3.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.once": "^4.1.6",
    "@types/ms": "^0.7.31",
    "@types/semver": "^7.3.4",
    "atob": "^2.1.2",
    "chai": "^4.1.2",
    "conventional-changelog": "~1.1.0",
    "cost-of-modules": "^1.0.1",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nsp": "^2.6.2",
    "nyc": "^11.9.0",
    "prettier": "^2.2.1",
    "sinon": "^6.0.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "npm": ">=1.4.28",
    "node": ">=4"
  },
  "files": [
    "dist"
  ]
}
